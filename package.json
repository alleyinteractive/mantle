    "@wordpress/url": "3.7.1",
    "@wordpress/viewport": "4.4.1",
    "@wordpress/warning": "2.6.1",
    "@wordpress/widgets": "2.4.8",
    "@wordpress/wordcount": "3.6.1",
    "classnames": "^2.3.1",
    "dompurify": "^2.3.4",
    "lodash": "4.17.21",
    "moment": "^2.29.4",
    "papaparse": "^5.3.1",
