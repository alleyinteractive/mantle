    "stylelint": "^14.11.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.7.2",
    "webpack-stats-plugin": "^1.0.3"
  }
