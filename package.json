{
  "name": "mantle",
  "private": true,
  "license": "GPL-2.0-or-later",
  "scripts": {
    "prebuild": "check-node-version --package",
    "build": "npm run production",
    "predev": "check-node-version --package",
    "dev": "npm run development",
    "development": "mix",
    "prehot": "check-node-version --package",
    "hot": "mix watch --hot",
    "preprod": "check-node-version --package",
    "prod": "npm run production",
    "production": "mix --production",
    "pretest:watch": "check-node-version --package",
    "test:watch": "jest --watch",
    "pretest": "check-node-version --package",
    "test": "jest",
    "preupdate-dependencies": "check-node-version --package",
    "update-dependencies": "node bin/update-dependencies.js",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "watch": "mix watch"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "modulePathIgnorePatterns": [
      ".buddy-tests"
    ]
  },
